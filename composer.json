{
    "name": "stephenjude/filament-debugger",
    "description": "About",
    "keywords": [
        "stephenjude",
        "laravel",
        "filament-debugger"
    ],
    "homepage": "https://github.com/stephenjude/filament-debugger",
    "license": "MIT",
    "authors": [
        {
            "name": "stephenjude",
            "email": "stephenjudesuccess@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^11.0",
        "spatie/laravel-package-tools": "^1.16.0"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Stephenjude\\FilamentDebugger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stephenjude\\FilamentDebugger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stephenjude\\FilamentDebugger\\DebuggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
